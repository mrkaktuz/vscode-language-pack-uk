<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="build/win32/i18n/messages" source-language="en" datatype="plaintext" target-language="uk">
    <body>
      <trans-unit id="AddContextMenuFiles">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer file context menu</source>
        <target>Додати дію "Відкрити з %1" до контекстного меню файлів Провідника Windows</target>
      </trans-unit>
      <trans-unit id="AddContextMenuFolders">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer directory context menu</source>
        <target>Додати дію "Відкрити з %1" до контекстного меню каталогу Провідника Windows</target>
      </trans-unit>
      <trans-unit id="AddToPath">
        <source xml:lang="en">Add to PATH (requires shell restart)</source>
        <target>Додати до PATH (потрібен перезапуск оболонки)</target>
      </trans-unit>
      <trans-unit id="AdditionalIcons">
        <source xml:lang="en">Additional icons:</source>
        <target>Додаткові значки:</target>
      </trans-unit>
      <trans-unit id="AssociateWithFiles">
        <source xml:lang="en">Register %1 as an editor for supported file types</source>
        <target>Зареєструйте %1 як редактора для підтримуваних типів файлів</target>
      </trans-unit>
      <trans-unit id="ConfirmUninstall">
        <source xml:lang="en">Are you sure you want to completely remove %1 and all of its components?</source>
        <target>Ви впевнені, що хочете повністю видалити %1 та всі його компоненти?</target>
      </trans-unit>
      <trans-unit id="CreateDesktopIcon">
        <source xml:lang="en">Create a &amp;desktop icon</source>
        <target>Створити значок &amp;на робочому столі</target>
      </trans-unit>
      <trans-unit id="CreateQuickLaunchIcon">
        <source xml:lang="en">Create a &amp;Quick Launch icon</source>
        <target>Створити значок &amp;Швидкого запуску</target>
      </trans-unit>
      <trans-unit id="FinishedLabel">
        <source xml:lang="en">Setup has finished installing [name] on your computer. The application may be launched by selecting the installed shortcuts.</source>
        <target>Встановлення [name] на вашому комп'ютері завершено. Додаток можна запустити, вибравши встановлені ярлики.</target>
      </trans-unit>
      <trans-unit id="OpenWithCodeContextMenu">
        <source xml:lang="en">Open w&amp;ith %1</source>
        <target>Відкрити з %1</target>
      </trans-unit>
      <trans-unit id="Other">
        <source xml:lang="en">Other:</source>
        <target>Інше:</target>
      </trans-unit>
      <trans-unit id="RunAfter">
        <source xml:lang="en">Run %1 after installation</source>
        <target>Запустіть %1 після встановлення</target>
      </trans-unit>
      <trans-unit id="SourceFile">
        <source xml:lang="en">%1 Source File</source>
        <target>%1 Файл джерела</target>
      </trans-unit>
      <trans-unit id="UpdatingVisualStudioCode">
        <source xml:lang="en">Updating Visual Studio Code...</source>
        <target>Оновлення Visual Studio Code...</target>
      </trans-unit>
    </body>
  </file>
</xliff>