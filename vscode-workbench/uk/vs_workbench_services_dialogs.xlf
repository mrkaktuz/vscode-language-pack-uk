  <file original="src/vs/workbench/services/dialogs/browser/abstractFileDialogService" source-language="en" datatype="plaintext" target-language="uk">
    <body>
      <trans-unit id="allFiles">
        <source xml:lang="en">All Files</source>
        <target>Усі файли</target>
      </trans-unit>
      <trans-unit id="dontSave">
        <source xml:lang="en">Do&amp;&amp;n't Save</source>
        <note>&amp;&amp; denotes a mnemonic</note>
        <target>Не&amp;&amp; зберігати</target>
      </trans-unit>
      <trans-unit id="filterName.workspace">
        <source xml:lang="en">Workspace</source>
        <target>Робоча область</target>
      </trans-unit>
      <trans-unit id="noExt">
        <source xml:lang="en">No Extension</source>
        <target>Без розширення</target>
      </trans-unit>
      <trans-unit id="openFile.title">
        <source xml:lang="en">Open File</source>
        <target>Відкрити файл</target>
      </trans-unit>
      <trans-unit id="openFileOrFolder.title">
        <source xml:lang="en">Open File or Folder</source>
        <target>Відкрити файл або папку</target>
      </trans-unit>
      <trans-unit id="openFolder.title">
        <source xml:lang="en">Open Folder</source>
        <target>Відкрити папку</target>
      </trans-unit>
      <trans-unit id="openWorkspace.title">
        <source xml:lang="en">Open Workspace from File</source>
        <target>Відкрити робочий простір з файлу</target>
      </trans-unit>
      <trans-unit id="save">
        <source xml:lang="en">&amp;&amp;Save</source>
        <note>&amp;&amp; denotes a mnemonic</note>
        <target>&amp;&amp;Зберегти</target>
      </trans-unit>
      <trans-unit id="saveAll">
        <source xml:lang="en">&amp;&amp;Save All</source>
        <note>&amp;&amp; denotes a mnemonic</note>
        <target>&amp;&amp;Зберегти все</target>
      </trans-unit>
      <trans-unit id="saveAsTitle">
        <source xml:lang="en">Save As</source>
        <target>Зберегти як</target>
      </trans-unit>
      <trans-unit id="saveChangesDetail">
        <source xml:lang="en">Your changes will be lost if you don't save them.</source>
        <target>Ваші зміни будуть втрачені, якщо ви їх не збережете.</target>
      </trans-unit>
      <trans-unit id="saveChangesMessage">
        <source xml:lang="en">Do you want to save the changes you made to {0}?</source>
        <target>Ви хочете зберегти зміни, які ви внесли до {0}?</target>
      </trans-unit>
      <trans-unit id="saveChangesMessages">
        <source xml:lang="en">Do you want to save the changes to the following {0} files?</source>
        <target>Ви хочете зберегти зміни до наступних {0} файлів?</target>
      </trans-unit>
      <trans-unit id="saveFileAs.title">
        <source xml:lang="en">Save As</source>
        <target>Зберегти як</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/services/dialogs/browser/fileDialogService" source-language="en" datatype="plaintext" target-language="uk">
    <body>
      <trans-unit id="learnMore">
        <source xml:lang="en">&amp;&amp;Learn More</source>
        <note>&amp;&amp; denotes a mnemonic</note>
        <target>&amp;&amp;Дізнатися більше</target>
      </trans-unit>
      <trans-unit id="openFiles">
        <source xml:lang="en">Open &amp;&amp;Files...</source>
        <note>&amp;&amp; denotes a mnemonic</note>
        <target>Відкрити &amp;&amp;Файли...</target>
      </trans-unit>
      <trans-unit id="openRemote">
        <source xml:lang="en">&amp;&amp;Open Remote...</source>
        <note>&amp;&amp; denotes a mnemonic</note>
        <target>&amp;&amp;Відкрити віддалене...</target>
      </trans-unit>
      <trans-unit id="pickFolderAndOpen">
        <source xml:lang="en">Can't open folders, try adding a folder to the workspace instead.</source>
        <target>Не вдається відкрити папки, спробуйте замість цього додати папку до робочого простору.</target>
      </trans-unit>
      <trans-unit id="pickWorkspaceAndOpen">
        <source xml:lang="en">Can't open workspaces, try adding a folder to the workspace instead.</source>
        <target>Не вдається відкрити робочі простори, спробуйте замість цього додати папку до робочого простору.</target>
      </trans-unit>
      <trans-unit id="unsupportedBrowserDetail">
        <source xml:lang="en">Your browser doesn't support opening local folders.
You can either open single files or open a remote repository.</source>
        <target>Ваш браузер не підтримує відкриття локальних папок. Ви можете або відкрити окремі файли, або відкрити віддалений репозиторій.</target>
      </trans-unit>
      <trans-unit id="unsupportedBrowserMessage">
        <source xml:lang="en">Opening Local Folders is Unsupported</source>
        <target>Відкриття локальних папок не підтримується</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/services/dialogs/browser/simpleFileDialog" source-language="en" datatype="plaintext" target-language="uk">
    <body>
      <trans-unit id="openLocalFile">
        <source xml:lang="en">Open Local File...</source>
        <target>Відкрити локальний файл...</target>
      </trans-unit>
      <trans-unit id="openLocalFileFolder">
        <source xml:lang="en">Open Local...</source>
        <target>Відкрити локальний...</target>
      </trans-unit>
      <trans-unit id="openLocalFolder">
        <source xml:lang="en">Open Local Folder...</source>
        <target>Відкрити локальну папку...</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.badPath">
        <source xml:lang="en">The path does not exist. Use ~ to go to your home directory.</source>
        <target>Шлях не існує. Використовуйте ~, щоб перейти до вашого домашнього каталогу.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.cancel">
        <source xml:lang="en">Cancel</source>
        <target>Скасувати</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.hideDotFiles">
        <source xml:lang="en">Hide dot files</source>
        <target>Сховати файли з крапкою</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.invalidPath">
        <source xml:lang="en">Please enter a valid path.</source>
        <target>Будь ласка, введіть дійсний шлях.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.local">
        <source xml:lang="en">Show Local</source>
        <target>Показати локально</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.notConnectedToRemote">
        <source xml:lang="en">File system provider for {0} is not available.</source>
        <target>Провайдер файлової системи для {0} недоступний.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.showDotFiles">
        <source xml:lang="en">Show dot files</source>
        <target>Показати файли з крапкою</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.validateBadFilename">
        <source xml:lang="en">Please enter a valid file name.</source>
        <target>Будь ласка, введіть дійсну назву файлу.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.validateCreateDirectory">
        <source xml:lang="en">The folder {0} does not exist. Would you like to create it?</source>
        <target>Тека {0} не існує. Бажаєте її створити?</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.validateExisting">
        <source xml:lang="en">{0} already exists. Are you sure you want to overwrite it?</source>
        <target>{0} вже існує. Ви впевнені, що хочете перезаписати його?</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.validateFileOnly">
        <source xml:lang="en">Please select a file.</source>
        <target>Будь ласка, виберіть файл.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.validateFolder">
        <source xml:lang="en">The folder already exists. Please use a new file name.</source>
        <target>Така папка вже існує. Будь ласка, використайте нову назву файлу.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.validateFolderOnly">
        <source xml:lang="en">Please select a folder.</source>
        <target>Будь ласка, виберіть папку.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.validateNonexistentDir">
        <source xml:lang="en">Please enter a path that exists.</source>
        <target>Будь ласка, введіть шлях, який існує.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.validateReadonlyFolder">
        <source xml:lang="en">This folder cannot be used as a save destination. Please choose another folder</source>
        <target>Цю папку не можна використовувати як місце для збереження. Будь ласка, виберіть іншу папку.</target>
      </trans-unit>
      <trans-unit id="remoteFileDialog.windowsDriveLetter">
        <source xml:lang="en">Please start the path with a drive letter.</source>
        <target>Будь ласка, почніть шлях з літери диска.</target>
      </trans-unit>
      <trans-unit id="saveLocalFile">
        <source xml:lang="en">Save Local File...</source>
        <target>Зберегти локальний файл...</target>
      </trans-unit>
    </body>
  </file>
