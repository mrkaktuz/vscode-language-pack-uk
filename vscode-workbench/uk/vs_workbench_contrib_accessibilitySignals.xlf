<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="src/vs/workbench/contrib/accessibilitySignals/browser/commands" source-language="en" datatype="plaintext" target-language="uk">
    <body>
      <trans-unit id="accessibility.announcement.help">
        <source xml:lang="en">Help: List Signal Announcements</source>
        <target>Допомога: Список оголошень сигналів</target>
      </trans-unit>
      <trans-unit id="accessibility.announcement.help.description">
        <source xml:lang="en">List all accessibility announcements, alerts, braille messages, and configure their settings</source>
        <target>Перерахуйте всі оголошення доступності, сповіщення, повідомлення шрифтом Брайля та налаштуйте їх параметри</target>
      </trans-unit>
      <trans-unit id="accessibility.sound.help.description">
        <source xml:lang="en">List all accessibility sounds, noises, or audio cues and configure their settings</source>
        <target>Перерахуйте всі звуки доступності, шуми або аудіосигнали та налаштуйте їх параметри</target>
      </trans-unit>
      <trans-unit id="announcement.help.placeholder">
        <source xml:lang="en">Select an announcement to configure</source>
        <target>Виберіть оголошення для налаштування</target>
      </trans-unit>
      <trans-unit id="announcement.help.placeholder.disabled">
        <source xml:lang="en">Screen reader is not active, announcements are disabled by default.</source>
        <target>Читач екрану не активний, оголошення за замовчуванням вимкнені.</target>
      </trans-unit>
      <trans-unit id="announcement.help.settings">
        <source xml:lang="en">Configure Announcement</source>
        <target>Налаштувати оголошення</target>
      </trans-unit>
      <trans-unit id="signals.sound.help">
        <source xml:lang="en">Help: List Signal Sounds</source>
        <target>Допомога: Список звукових сигналів</target>
      </trans-unit>
      <trans-unit id="sounds.help.placeholder">
        <source xml:lang="en">Select a sound to play and configure</source>
        <target>Виберіть звук для відтворення та налаштування</target>
      </trans-unit>
      <trans-unit id="sounds.help.settings">
        <source xml:lang="en">Configure Sound</source>
        <target>Налаштувати звук</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/accessibilitySignals/browser/openDiffEditorAnnouncement" source-language="en" datatype="plaintext" target-language="uk">
