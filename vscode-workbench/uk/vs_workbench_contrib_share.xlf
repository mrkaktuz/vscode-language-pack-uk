  <file original="src/vs/workbench/contrib/share/browser/share.contribution" source-language="en" datatype="plaintext" target-language="uk">
    <body>
      <trans-unit id="close">
        <source xml:lang="en">Close</source>
        <target>Закрити</target>
      </trans-unit>
      <trans-unit id="experimental.share.enabled">
        <source xml:lang="en">Controls whether to render the Share action next to the command center when {0} is {1}.</source>
        <target>Контролює, чи відображати дію Поділитися поруч із командним центром, коли {0} є {1}.</target>
      </trans-unit>
      <trans-unit id="generating link">
        <source xml:lang="en">Generating link...</source>
        <target>Генерація посилання...</target>
      </trans-unit>
      <trans-unit id="open link">
        <source xml:lang="en">Open Link</source>
        <target>Відкрити посилання</target>
      </trans-unit>
      <trans-unit id="share">
        <source xml:lang="en">Share...</source>
        <target>Поділитися...</target>
      </trans-unit>
      <trans-unit id="shareSuccess">
        <source xml:lang="en">Copied link to clipboard!</source>
        <target>Скопійовано посилання в буфер обміну!</target>
      </trans-unit>
      <trans-unit id="shareTextSuccess">
        <source xml:lang="en">Copied text to clipboard!</source>
        <target>Скопійовано текст в буфер обміну!</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/share/browser/shareService" source-language="en" datatype="plaintext" target-language="uk">
    <body>
      <trans-unit id="shareProviderCount">
        <source xml:lang="en">The number of available share providers</source>
        <target>Кількість доступних постачальників акцій</target>
      </trans-unit>
      <trans-unit id="toggle.share">
        <source xml:lang="en">Share</source>
        <target>Поділитися</target>
      </trans-unit>
      <trans-unit id="toggle.shareDescription">
        <source xml:lang="en">Toggle visibility of the Share action in title bar</source>
        <target>Перемикати видимість дії "Поділитися" у заголовку</target>
      </trans-unit>
      <trans-unit id="type to filter">
        <source xml:lang="en">Choose how to share {0}</source>
        <target>Виберіть, як поділитися {0}</target>
      </trans-unit>
    </body>
  </file>
